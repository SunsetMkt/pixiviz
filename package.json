{
  "name": "pixiviz",
  "version": "1.27.0",
  "scripts": {
    "serve": "node ./tools/processVersion.js && vue-cli-service serve",
    "build": "node ./tools/processVersion.js && vue-cli-service build",
    "test:e2e": "vue-cli-service test:e2e",
    "analyze": "cross-env ANALYZE=true vue-cli-service build",
    "dev": "npm run serve",
    "dev:built": "npm run build && http-server ./dist -b",
    "gen:anno": "cross-env NODE_ENV=prod node ./tools/announcementGen.js",
    "gen:anno-dev": "cross-env NODE_ENV=dev node ./tools/announcementGen.js",
    "gen:proxy": "node ./tools/proxyConfigGen.js",
    "gen:ver": "node ./tools/processVersion.js",
    "push:anno": "tigo cfs deploy pixiviz-anno-prod",
    "push:anno-dev": "tigo cfs deploy pixiviz-anno-dev",
    "push:proxy": "cross-env NODE_ENV=prod node ./tools/proxyConfigUpload.js",
    "push:proxy-dev": "cross-env NODE_ENV=dev node ./tools/proxyConfigUpload.js",
    "push:config": "tigo cfs deploy pixiviz-prod",
    "start": "npm run serve",
    "upload:anno": "npm run push:anno",
    "upload:anno-dev": "npm run push:anno-dev",
    "upload:proxy": "npm run push:proxy",
    "upload:proxy-dev": "npm run push:proxy-dev",
    "f2elint-scan": "f2elint scan",
    "f2elint-fix": "f2elint fix",
    "compress-svg": "npx svgo ./src/assets/icons"
  },
  "dependencies": {
    "@backrunner/qrcode-svg": "^1.2.0",
    "@better-scroll/core": "^2.4.2",
    "@pwp-app/better-element-ui": "^2.15.7",
    "@pwp-app/vue-context-menu": "^1.3.1",
    "@pwp-app/vue-lazyload": "^1.3.4",
    "@vue/composition-api": "^1.4.9",
    "axios": "^0.28.0",
    "clipboard-polyfill": "^3.0.3",
    "core-js": "^3.21.1",
    "crc": "^4.1.0",
    "dayjs": "^1.10.8",
    "hash-wasm": "^4.9.0",
    "jszip": "^3.8.0",
    "level-js": "^6.1.0",
    "levelup": "^5.1.1",
    "lodash-es": "^4.17.21",
    "pixland": "^0.0.8",
    "qs": "^6.10.4",
    "regex-go": "^0.4.0",
    "register-service-worker": "^1.7.2",
    "seedrandom": "^3.0.5",
    "vue": "^2.6.14",
    "vue-cookies": "^1.7.4",
    "vue-infinite-loading": "^2.4.5",
    "vue-router": "^3.5.3",
    "vue-share-popup": "^0.4.3",
    "vue-touch": "^2.0.0-beta.4",
    "vue2-touch-events": "^3.2.2",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.17.0",
    "@gfx/zopfli": "^1.0.15",
    "@vue/cli-plugin-babel": "^5.0.1",
    "@vue/cli-plugin-e2e-cypress": "^5.0.1",
    "@vue/cli-plugin-pwa": "^5.0.1",
    "@vue/cli-plugin-router": "^5.0.1",
    "@vue/cli-plugin-vuex": "^5.0.1",
    "@vue/cli-service": "^5.0.1",
    "babel-plugin-component": "^1.1.1",
    "brotli-webpack-plugin": "^1.1.0",
    "compression-webpack-plugin": "^9.2.0",
    "cross-env": "^7.0.3",
    "cypress": "^13.11.0",
    "f2elint": "^4.7.1",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "postcss": "^8.4.31",
    "qiniu": "^7.4.0",
    "vue-cli-plugin-element": "^1.0.1",
    "vue-template-compiler": "^2.6.14"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "last 3 Android versions",
    "last 3 iOS versions",
    "not IE 11"
  ]
}
